{
  "env": {
      "browser": true,
      "es2021": true,
      "node": true
  },
  "extends": [
      "plugin:react/recommended",
      "plugin:jsx-a11y/recommended"

  ],
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "allowImportExportEverywhere": true,
      "ecmaFeatures": {
          "jsx": true
      },
      "rules": {
        "semi": "error"
    }

  },
  "plugins": [
      "react",
      "jsx-a11y"
  ],
  "rules": {
      "react/prop-types": "off"
  }
}
